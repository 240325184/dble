<!DOCTYPE dble:sharding SYSTEM "sharding.dtd">
<dble:sharding xmlns:dble="http://dble.cloud/">

    <schema name="testdb"  sqlMaxLimit="100">
        <shardingTable name="tb_enum_sharding" shardingNode="dn1,dn2" function="func_enum" shardingColumn="id" />
        <shardingTable name="sharding_4_t1" shardingNode="dn1,dn2" function="func_enum" shardingColumn="id" />

    </schema>
    <schema name="ss"  sqlMaxLimit="100">
        <shardingTable name="tb_enum_sharding" shardingNode="dn1,dn2" function="func_enum" shardingColumn="id" />
        <shardingTable name="sharding_4_t1" shardingNode="dn1,dn2" function="func_enum" shardingColumn="id" />

    </schema>

    <schema shardingNode="dn1" name="schema1" sqlMaxLimit="100">
        <singleTable name="single_t1" shardingNode="dn1" />
        <globalTable name="global_t1" shardingNode="dn1,dn2" />
    </schema>

    <schema name="schema2" sqlMaxLimit="100">
        <singleTable name="single_t3" shardingNode="dn1" />
    </schema>
    <shardingNode name="dn1" dbGroup="ha_group2" database="db_1"/>
    <shardingNode name="dn2" dbGroup="ha_group2" database="db_3"/>

    <function name="func_enum" class="Enum">
        <property name="mapFile">partition-enum.txt</property>
        <property name="defaultNode">0</property><!--the default is -1,means unexpected value will report error-->
        <property name="type">0</property><!--0 means key is a number, 1 means key is a string-->
    </function>
</dble:sharding>